<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>fr.openwide.core.examples</groupId>
		<artifactId>owsi-core-examples</artifactId>
		<version>0.7.3-SNAPSHOT</version>
	</parent>

	<artifactId>owsi-core-example-export</artifactId>

	<name>OWSI Core - Example - Export PDF/XLS</name>
	<description>Capitalisation OWSI des d√©veloppements d'exports PDF et Excel</description>
	
	<dependencies>
		<dependency>
			<groupId>fr.openwide.core.components</groupId>
			<artifactId>owsi-core-component-export</artifactId>
			<version>${project.version}</version>
		</dependency>
		
		<dependency>
			<groupId>fr.openwide.core.components</groupId>
			<artifactId>owsi-core-component-commons</artifactId>
			<version>${project.version}</version>
		</dependency>
		
		<dependency>
			<groupId>fr.openwide.core.components</groupId>
			<artifactId>owsi-core-component-jpa</artifactId>
			<version>${project.version}</version>
		</dependency>
		
		<!-- Export PDF -->
		<dependency>
			<groupId>com.lowagie</groupId>
			<artifactId>itext</artifactId>
			<version>${owsi-core.itext.version}</version>
			<exclusions>
				<exclusion>
					<groupId>bouncycastle</groupId>
					<artifactId>bcmail-jdk14</artifactId>
				</exclusion>
				<exclusion>
					<groupId>org.bouncycastle</groupId>
					<artifactId>bctsp-jdk14</artifactId>
				</exclusion>
				<exclusion>
					<groupId>bouncycastle</groupId>
					<artifactId>bcprov-jdk14</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		
		<!-- Export Excel -->
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi</artifactId>
			<version>${owsi-core.poi.version}</version>
		</dependency>
		
		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>javax.servlet-api</artifactId>
			<version>${owsi-core.servlet-api.version}</version>
			<scope>provided</scope>
		</dependency>
		
		<!-- Logging -->
		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>${owsi-core.log4j.version}</version>
		</dependency>
	</dependencies>
</project>
