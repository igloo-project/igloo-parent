	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-enforcer-plugin</artifactId>
					<executions>
					<execution>
					<id>enforce</id>
					<goals>
						<goal>enforce</goal>
					</goals>
						<configuration>
							<rules>
								<requireUpperBoundDeps >
									<!-- hibernate/mockito conflict; let hibernate decides! -->
									<excludes combine.children="append">
										<exclude>net.bytebuddy:byte-buddy</exclude>
									</excludes>
								</requireUpperBoundDeps>
							</rules>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

