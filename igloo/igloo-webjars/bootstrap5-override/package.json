{
  "name": "bootstrap5-override",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm-run-all --parallel watch docs-serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "js": "npm-run-all js-compile",
    "js-compile": "npm-run-all js-compile-*",
    "js-compile-modal-more": "rollup --environment SYMBOL:ModalMore --environment FILE:modal-more --config ./rollup.config.mjs --sourcemap",
    "js-compile-offcanvas-more": "rollup --environment SYMBOL:OffcanvasMore --environment FILE:offcanvas-more --config ./rollup.config.mjs --sourcemap",
    "js-compile-tooltip-more": "rollup --environment SYMBOL:TooltipMore --environment FILE:tooltip-more --config ./rollup.config.mjs --sourcemap",
    "js-compile-confirm": "rollup --environment SYMBOL:Confirm --environment FILE:confirm --config ./rollup.config.mjs --sourcemap",
    "js-compile-tab-more": "rollup --environment SYMBOL:Tab --environment FILE:tab-more --config ./rollup.config.mjs --sourcemap",
    "docs-serve": "hugo server --port 9001 --disableFastRender",
    "watch": "npm-run-all --parallel watch-*",
    "watch-js-main": "nodemon --watch src/main/js --ext js --exec \"npm-run-all js-compile\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
  },
  "peerDependencies": {
    "bootstrap": "5.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve" : "^16.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@babel/preset-env": "^7.26.9",
    "@babel/core": "^7.26.10",
    "hugo-bin": "^0.142.0",
    "rollup": "^4.39.0",
    "npm-run-all": "^4.1.5",
    "nodemon": "^3.1.9"
  },
  "hugo-bin": {
    "buildTags": "extended"
  }
}
